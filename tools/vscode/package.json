{
  "name": "protocols",
  "displayName": "Protocols",
  "description": "Syntax highlighting for Protocols language (.prot and .tx files)",
  "version": "0.0.1",
  "publisher": "protocols",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cucapra/protocols"
  },
  "contributes": {
    "languages": [
      {
        "id": "protocols",
        "aliases": [
          "Protocols",
          "protocols"
        ],
        "extensions": [
          ".prot"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "protocols-transactions",
        "aliases": [
          "Protocols Transactions",
          "transactions"
        ],
        "extensions": [
          ".tx"
        ],
        "configuration": "./transactions-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "protocols",
        "scopeName": "source.protocols",
        "path": "./syntaxes/protocols.tmLanguage.json"
      },
      {
        "language": "protocols-transactions",
        "scopeName": "source.protocols.transactions",
        "path": "./syntaxes/transactions.tmLanguage.json"
      }
    ]
  }
}