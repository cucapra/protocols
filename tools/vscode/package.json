{
  "name": "protocols",
  "displayName": "Protocols Language Syntax Highlighting",
  "description": "Syntax highlighting for the Protocols language",
  "version": "0.0.1",
  "publisher": "protocols",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cucapra/protocols"
  },
  "contributes": {
    "languages": [
      {
        "id": "protocols",
        "aliases": [
          "Protocols"
        ],
        "extensions": [
          ".prot"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "protocols",
        "scopeName": "source.protocols",
        "path": "./syntaxes/protocols.tmLanguage.json"
      }
    ]
  }
}