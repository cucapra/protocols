WHITESPACE = _{ " " | NEWLINE }
id = @{ ASCII_ALPHA  ~ ASCII_ALPHANUMERIC* }
path_id = @{ id ~ ("." ~ id)* }
tpe = {"u32"}
assign = {path_id ~ ":=" ~ path_id }
cmd = { id ~ "(" ~ ")" }
stmt = { (assign | cmd) ~ ";" }
dir = _{"in" | "out" }
arg = { dir ~ id ~ ":" ~ tpe }
arglist = { (arg ~ "," ~ arglist) | arg }
fun = { "fn" ~ id ~ "(" ~ arglist ~ ")" ~ "{" ~ stmt* ~ "}" }
file = { SOI ~ fun* ~ EOI }