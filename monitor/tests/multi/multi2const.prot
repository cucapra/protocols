// ARGS: --wave multi/multi2const.fst --instances multi2const:multi2const
struct multi2const {
    in start: u1,
    in inp: u64,
    out out: u64,
}

fn multi<DUT: multi2const>(in data_in: u64, out data_out: u64) {
    DUT.start := 1'b1;
    DUT.inp := data_in;
    
    step();

    DUT.start := 1'b0;
    DUT.inp := X;

    step(3);

    assert_eq(data_out, DUT.out);

    fork();
    step();
}