// ARGS: --wave brave_new_world_francis/bit_truncation_sha.fst --instances bit_truncation_sha_fixed:bit_truncation_sha
struct bit_truncation_sha {
    in right: u64,
    out left: u42,
}

fn bit_truncation_sha<dut: bit_truncation_sha>(in data_in: u64, out data_out: u42) {
    dut.right := data_in;
    step();
    assert_eq(data_out, dut.left);
    fork();
    step();
}