// ARGS: --wave brave_new_world_francis/bit_truncation_fft.fst --instances round11_rne_unsigned_fixed:BitTruncationFFT
struct BitTruncationFFT {
    in input_data: u27,
    out output_data: u11,
}

prot bit_truncation_fft<dut: BitTruncationFFT>(in data_in: u27, out data_out: u11) {
    dut.input_data := data_in;
    step();
    assert_eq(data_out, dut.output_data);
    fork();
    step();
}