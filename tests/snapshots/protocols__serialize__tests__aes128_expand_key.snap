struct ExpandKey128 {
  in input: u128,
  out output: u128,
  out output_delayed: u128,
}

fn aes128<dut: ExpandKey128>(in input: u128, out output: u128) {
  dut.input := input;
  step(1);
  fork();
  dut.input := X;
  assert_eq(dut.output, output);
  step(1);
  assert_eq(dut.output_delayed, output);
}
