struct multi2const {
    in start: u1,
    in inp: u64,
    out out: u64,
}

fn multi<dut: multi2const>(in data_in: u64, out data_out: u64, in zero: u1, in one: u1) {
    dut.start := one;
    dut.inp := data_in;
    
    step();

    dut.start := zero;
    dut.inp := X;

    step(3);

    assert_eq(dut.out, data_out);
}