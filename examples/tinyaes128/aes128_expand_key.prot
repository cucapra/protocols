struct ExpandKey128 {
  in input: u128,
  out output: u128,
  out output_delayed: u128,
}

prot aes128<dut : ExpandKey128>(in input: u128, out output: u128) {
  dut.input := input;
  step();
  fork();
  dut.input := X;
  assert_eq(dut.output, output);
  step();
  assert_eq(dut.output_delayed, output);
}