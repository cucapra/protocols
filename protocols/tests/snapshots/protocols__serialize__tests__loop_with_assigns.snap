struct Adder {
  in a: u32,
  in b: u32,
  out s: u32,
}

fn loop_add<DUT: Adder>(in a: u32, in b: u32, in num_iters: u8, out s: u32) {
  repeat num_iters iterations {
    DUT.a := a;
    DUT.b := b;
    step();
    DUT.a := X;
    DUT.b := X;
    assert_eq(s, DUT.s);
  }
  fork();
  step();
}
