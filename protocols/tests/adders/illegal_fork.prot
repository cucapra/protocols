// This protocol does not correspond to a specific verilog module, as it's never executed.

struct Adder {
  in a: u32,
  in b: u32,
  out s: u32,
}

fn add<DUT: Adder>(in a: u32, in b: u32, out s: u32) {
  DUT.a := a;
  DUT.b := b;
  step();
  fork(1); // forks cannot take an argument - will fail
  DUT.a := X;
  DUT.b := X;
  s := DUT.s;
  DUT.s := a;
  assert_eq(s, DUT.s);
}
