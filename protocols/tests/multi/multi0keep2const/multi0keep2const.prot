struct multi0keep2const {
    in start: u1,
    in inp: u64,
    out out: u64,
}

fn multi<dut: multi0keep2const>(in data_in: u64, out data_out: u64) {
    dut.start := 1'b1;
    dut.inp := data_in;
    
    step();

    dut.start := 1'b0;
    dut.inp := X;

    step(3);

    assert_eq(dut.out, data_out);

    fork();
    step();
}